<?xml version="1.0" encoding="UTF-8" ?>
<configuration>
 	<appender name="Console" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%date [%thread] %-5level %logger{80} - %msg%n</pattern>
        </encoder>
    </appender>
    <root level="debug">
        <appender-ref ref="Console" />
    </root>
    
 </configuration>